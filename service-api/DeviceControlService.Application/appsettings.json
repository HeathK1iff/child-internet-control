{
  "Kestrel": {
    "Endpoints": {
      "Http":{
        "Url": "http://+:8081"
      }
    }
  },
  "Auth": {
    "Key": ""
  },
  "AllowedHosts": "*",
  "Router": {
    "Host": "locahost",
    "Credential": {
      "Login": "user",
      "Password": "****"
    }
  },
  "Serilog": {
    "MinimumLevel": {
          "Default": "Information", 
          "Override": {
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information",
            "Microsoft.AspNetCore": "Warning"
          }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/app.log", 
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}